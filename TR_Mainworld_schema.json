{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"Header": {
			"type": "object",
			"properties": {
				"Object Type": {
					"type": "string"
				},
				"Rules System": {
					"type": "string"
				},
				"Edition": {
					"type": "integer"
				},
				"Version": {
					"type": "integer"
				},
				"Extensions": {
					"type": "array",
					"items": [
						{
							"type": "object"
						}
					]
				}
			},
			"required": [
				"Object Type",
				"Rules System",
				"Edition",
				"Version",
				"Extensions"
			]
		},
		"Contents": {
			"type": "object",
			"properties": {
				"Name": {
					"type": "string"
				},
				"UWP": {
					"type": "string"
				},
				"Starport": {
					"type": "string"
				},
				"Size": {
					"type": "integer"
				},
				"Atmosphere": {
					"type": "integer"
				},
				"Hydrographics": {
					"type": "integer"
				},
				"Population": {
					"type": "integer"
				},
				"Goverment": {
					"type": "integer"
				},
				"Law Level": {
					"type": "integer"
				},
				"Tech Level": {
					"type": "integer"
				},
				"Bases": {
					"type": "string"
				},
				"Trade Codes": {
					"type": "array",
					"items": [
						{
							"type": "string"
						},
						{
							"type": "string"
						}
					]
				},
				"Planetoid Belts": {
					"type": "boolean"
				},
				"Gas Giants": {
					"type": "boolean"
				},
				"Allegiance": {
					"type": "string"
				},
				"Extension Data": {
					"type": "object"
				}
			},
			"required": [
				"Name",
				"UWP",
				"Starport",
				"Size",
				"Atmosphere",
				"Hydrographics",
				"Population",
				"Goverment",
				"Law Level",
				"Tech Level",
				"Bases",
				"Trade Codes",
				"Planetoid Belts",
				"Gas Giants",
				"Allegiance",
				"Extension Data"
			]
		}
	},
	"required": [
		"Header",
		"Contents"
	]
}